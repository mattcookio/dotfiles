{
  "env": {
    "CLAUDE_CODE_EXPERIMENTAL_AGENT_TEAMS": "1"
  },
  "hooks": {
    "PreToolUse": [
      {
        "matcher": "Bash",
        "hooks": [
          {
            "type": "command",
            "command": "$HOME/.config/claude/hooks/git-hygiene-check.sh",
            "timeout": 10
          }
        ]
      }
    ]
  },
  "outputStyle": "experienced-developer",
  "skipDangerousModePermissionPrompt": true
}
